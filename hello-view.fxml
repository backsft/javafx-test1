<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="606.0" prefWidth="768.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="com.example.javafxpagesetup.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="text" prefHeight="47.0" prefWidth="377.0">
      <font>
         <Font size="25.0" />
      </font>
   </TextField>

    <Label fx:id="labelshow" prefHeight="51.0" prefWidth="360.0">
      <font>
         <Font size="19.0" />
      </font></Label>
    <Button onAction="#onHelloButtonClick" text="submit" />
</VBox>
